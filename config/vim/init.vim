runtime! parts/airline.vim
runtime! parts/coc.vim
runtime! parts/plugins.vim
runtime! parts/keybinds.vim
runtime! parts/fzf.vim

syntax on
highlight Normal ctermbg=NONE

" Use theme
" set termguicolors
" colorscheme dracula
" colorscheme monokai 

" Show number lines by default
set number
set cindent
set visualbell

" Set indent
set expandtab
set smarttab
set shiftwidth=2
set softtabstop=2
set tabstop=2

" Prettier auto format
" let g:prettier#autoformat = 1
